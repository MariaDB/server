#!/usr/bin/make -f
# -*- makefile-gmake -*-
#
# Uncomment this to turn on verbose mode.
#export DH_VERBOSE=1
# This has to be exported to make some magic below work.
export DH_OPTIONS

%:
	dh $@

override_dh_install:
	install -d debian/tmp/usr/share/keyrings/
	install -m 0644 groonga-keyring.gpg \
	  debian/tmp/usr/share/keyrings/

	dh_install
