DROP TABLE IF EXISTS scores;
SET NAMES UTF8;
CREATE TABLE scores (
id BIGINT(20) PRIMARY KEY AUTO_INCREMENT NOT NULL,
name CHAR(30) NOT NULL,
score INT NOT NULL,
KEY property (name, score)
) DEFAULT CHARSET=UTF8;
