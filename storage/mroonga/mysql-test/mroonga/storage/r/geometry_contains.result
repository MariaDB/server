DROP TABLE IF EXISTS shops;
CREATE TABLE shops (
id INT PRIMARY KEY AUTO_INCREMENT,
name TEXT,
location GEOMETRY NOT NULL,
SPATIAL KEY location_index (location)
);
SHOW CREATE TABLE shops;
Table	Create Table
shops	CREATE TABLE `shops` (
