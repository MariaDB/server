DROP TABLE IF EXISTS t1;
set @save_time_zone=@@time_zone;
set time_zone='UTC';
CREATE TABLE t1 (
d DATE ,
dt DATETIME ,
ts TIMESTAMP ,
t TIME ,
y YEAR ,
y4 YEAR(4) ,
y2 YEAR(2) ,
pk DATETIME PRIMARY KEY
) ENGINE=rocksdb;
