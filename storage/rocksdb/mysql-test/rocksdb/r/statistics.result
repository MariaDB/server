create table t1(
id bigint not null primary key auto_increment,
a varchar(255) not null,
b bigint,
index t1_1(b)
) engine=rocksdb;
create table t2(
id bigint not null primary key auto_increment,
a varchar(255) not null,
b bigint,
index t2_1(b) comment 'cf_t3'
) engine=rocksdb;
create table t3(
id bigint not null primary key auto_increment,
a varchar(255) not null,
b bigint,
index t3_1(b) comment 'rev:cf_t4'
) engine=rocksdb;
