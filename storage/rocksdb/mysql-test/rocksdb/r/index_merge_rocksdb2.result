#
CREATE TABLE t1 (
id INT NOT NULL PRIMARY KEY,
id2 INT NOT NULL,
id3 INT NOT NULL,
KEY (id2),
KEY (id3),
KEY covering_index (id2,id3)
) ENGINE=RocksDB;
