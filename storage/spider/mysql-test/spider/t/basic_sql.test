  SELECT a, b, c FROM tb_l;
  eval CREATE TABLE ta_l (
    a INT,
    b CHAR(1),
    c DATETIME,
    PRIMARY KEY(a)
  ) $MASTER_1_ENGINE $MASTER_1_CHARSET $MASTER_1_COMMENT_2_1;
  INSERT INTO ta_l SELECT a, b, c FROM tb_l;
}
if (!$MASTER_1_MERGE)
{
  eval CREATE TABLE ta_l (
    PRIMARY KEY(a)
  ) $MASTER_1_ENGINE $MASTER_1_CHARSET $MASTER_1_COMMENT_2_1
  SELECT a, b, c FROM tb_l;
}
