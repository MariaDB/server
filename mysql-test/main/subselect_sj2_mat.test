set optimizer_switch='materialization=off';

EXPLAIN EXTENDED
SELECT * FROM t1 LEFT JOIN t2 ON ( b1 = a2 ) 
  WHERE ( b1, b1 )  IN ( SELECT a4, b4 FROM t3, t4);
