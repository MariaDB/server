  DECLARE CONTINUE HANDLER FOR NOT FOUND SET done=TRUE;
  CREATE TABLE t2 (a INT, b VARCHAR(10));
  OPEN c(a_a);
  read_loop: LOOP
    FETCH c INTO v_a, v_b;
    IF done THEN
      LEAVE read_loop;
    END IF;
    INSERT INTO t2 VALUES (v_a,v_b);
  END LOOP;
  CLOSE c;
  SELECT * FROM t2;
  DROP TABLE t2;
END;
$$
DELIMITER ;$$
CALL p1(1);
CALL p1(3);
CALL p1(6);
DROP TABLE t1;
DROP PROCEDURE p1;



