  SET rec2.a= 11;
  SET rec2.b= 12;
  SET rec3.a= 11;
  SET rec3.b= 12;
  SELECT rec1=rec2, rec2=rec1, rec2=rec3, rec3=rec2;
END;
$$
DELIMITER ;$$
CALL p1();
DROP PROCEDURE p1;


