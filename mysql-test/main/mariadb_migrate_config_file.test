--exec $MARIADB_MIGRATE_CONFIG_FILE --defaults-file=$MYSQLTEST_VARDIR/tmp/tmp1.cnf --basedir=$MARIADBD_PATH --update --error-group=mysqld-5.7 --edit=inline --from=mariadb
--exec cat $MYSQLTEST_VARDIR/tmp/tmp1.cnf
