--echo #

SET NAMES utf8mb3 COLLATE utf8mb3_unicode_ci /*Unicode-4.0 folding*/;
--source include/ctype_casefolding.inc

SET NAMES utf8mb3 COLLATE utf8mb3_unicode_520_ci;
