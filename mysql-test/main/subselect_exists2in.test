CREATE ALGORITHM=MERGE VIEW v AS SELECT * FROM t2 WHERE b < 1;

SELECT c FROM v  
WHERE EXISTS ( 
  SELECT * FROM t1, t2 
  WHERE a <= v.d AND b = v.b 
);

