create procedure p1()
begin
  declare i int;
  set i = 0;
  while i < 4 do
    insert into t1
      with cte(a) as (select i) select r.a-1, s.a+1 from cte as r, cte as s;
    set i = i+1;
  end while;
end|

create procedure p2(in i int)
begin
 insert into t1
   with cte(a) as (select i) select r.a-1, s.a+1 from cte as r, cte as s;
end|

