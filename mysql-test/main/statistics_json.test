--echo #
--disable_warnings
drop table if exists t1;
--enable_warnings

set @save_histogram_type=@@histogram_type;
set @save_histogram_size=@@histogram_size;

CREATE TABLE t1 (
    a int NOT NULL PRIMARY KEY,
    b varchar(32),
    c char(16),
    d date,
    e double,
    f bit(3),
    INDEX idx1 (b, e),
    INDEX idx2 (c, d),
    INDEX idx3 (d),
    INDEX idx4 (e, b, d)
);

