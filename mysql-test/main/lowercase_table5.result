#
# MDEV-33084 LASTVAL(t1) and LASTVAL(T1) do not work well with lower-case-table-names=0
#
CREATE SEQUENCE t1;
CREATE SEQUENCE T1;
SELECT nextval(t1), lastval(t1);
nextval(t1)	lastval(t1)
1	1
SELECT nextval(T1), lastval(T1);
nextval(T1)	lastval(T1)
1	1
SELECT lastval(t1), lastval(T1) l2;
lastval(t1)	l2
1	1
DROP SEQUENCE t1, T1;
