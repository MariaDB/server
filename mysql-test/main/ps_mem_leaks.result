BEGIN
DECLARE va INT DEFAULT 0;
DECLARE cur CURSOR FOR SELECT a FROM t1;
OPEN cur;
FETCH cur INTO va;
SELECT va;
CLOSE cur;
END;
$
CALL p1;
va
1
ALTER TABLE t1 MODIFY a INT UNSIGNED;
CALL p1;
va
1
