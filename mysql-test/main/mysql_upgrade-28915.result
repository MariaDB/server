sp1	BEGIN
SELECT val1 FROM t1;
END	BEGIN
SELECT val1 FROM t1;
END
CALL sp1();
val1
SELECT name, body_utf8, body FROM mysql.proc WHERE name like 'sp1';
name	body_utf8	body
sp1	BEGIN
SELECT val1 FROM t1;
END	BEGIN
SELECT val1 FROM t1;
END
SET GLOBAL event_scheduler=ON;
SELECT name, body_utf8, body FROM mysql.event;
