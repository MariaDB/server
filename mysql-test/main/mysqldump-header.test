--source include/not_embedded.inc

CREATE TABLE `courses` (
  `course_id` smallint(20) PRIMARY KEY,
  `name` varchar(50),
  `description` varchar(100),
  `num_years` tinyint(1),
  `escape_çÇÁá!#%"'` varchar(10)
);

desc courses;

