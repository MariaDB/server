SELECT @@version RLIKE 'unknown|experimental|alpha|beta|gamma' AS MATURITY_INDICATED;
SELECT @@version RLIKE 'debug' AS DEBUG_BUILD;
SELECT version() RLIKE 'unknown|experimental|alpha|beta|gamma' AS MATURITY_INDICATED;
SELECT version() RLIKE 'debug' AS DEBUG_BUILD;