CREATE TABLE `t11` (
  id INT
) ENGINE=InnoDB ENCRYPTED="YES" PARTITION BY RANGE(id)
SUBPARTITION BY HASH(id) (
  PARTITION pt1 VALUES LESS THAN (100) ENCRYPTED="NO" (
