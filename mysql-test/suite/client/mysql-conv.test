--character_set latin1

# default encoding
--exec echo "t1" | $MYSQL_CONV
--exec echo "t1" | $MYSQL_CONV -f filename
--exec echo "t1" | $MYSQL_CONV -t filename

# invalid option
--replace_regex /.*mysql-conv.*: unknown/mysql-conv: unknown/
--error 1
--exec echo "t1" | $MYSQL_CONV -f filename -r latin1  2>&1 > /dev/null

# no arguments
--replace_regex /.*mysql-conv.*: unknown/mysql-conv: unknown/
--error 1
--exec $MYSQL_CONV 2>&1 > /dev/null
