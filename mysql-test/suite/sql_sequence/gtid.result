not support create table engine=sequence.
###########################################
connection master;
create table t(id int)engine=sequence;
ERROR 42000: Unknown storage engine 'sequence'
create table t(id int)engine=innodb;
alter table t engine=sequence;
ERROR 42000: Unknown storage engine 'sequence'
drop table t;
