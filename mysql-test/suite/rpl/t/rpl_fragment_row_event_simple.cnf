!include ../my.cnf

[mysqld]
max_allowed_packet=1KB
net_buffer_length=1KB

[mysqlbinlog]
debug_binlog_row_event_max_encoded_size=1KB

[mysqld.1]
gtid_domain_id=0
server_id=1

[mysqld.2]
gtid_domain_id=1
server_id=2
log_slave_updates

[mysqld.3]
gtid_domain_id=2
server_id=3
log_slave_updates

[ENV]
SERVER_MYPORT_3=		@mysqld.3.port
SERVER_MYSOCK_3=		@mysqld.3.socket
