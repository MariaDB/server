DELIMITER $$;
CREATE PROCEDURE p1(a_a INT, a_b VARCHAR)
AS
  v_a INT;
  v_b VARCHAR(10);
  CURSOR c (p_a INT, p_b VARCHAR) IS SELECT p_a,p_b FROM DUAL;
BEGIN
  FOR i IN 0..1
  LOOP
    OPEN c(a_a + i,a_b);
    LOOP
      FETCH c INTO v_a, v_b;
      EXIT WHEN c%NOTFOUND;
      SELECT 'Fetched a record a=' || v_a || ' b=' || v_b AS msg;
    END LOOP;
    CLOSE c;
  END LOOP;
END;
$$
DELIMITER ;$$
CALL p1(1,'b1');
DROP PROCEDURE p1;


