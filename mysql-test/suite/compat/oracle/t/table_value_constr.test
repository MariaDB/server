--echo #
--echo # MDEV-15940: cursor over TVC
--echo #

DELIMITER |;

DECLARE
  v INT;
  CURSOR cur IS VALUES(7);
BEGIN
  OPEN cur;
  FETCH cur INTO v;
  SELECT v;
END;
|

DECLARE
  v INT DEFAULT 0;
BEGIN
  FOR a IN (VALUES (7)) LOOP
    SET v = v + 1;
  END LOOP;
  SELECT v;
END;
|

DELIMITER ;|

