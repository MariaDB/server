CREATE FUNCTION f1 (lower_bound INT, upper_bound INT, lim INT) RETURN INT
AS
  total INT := 0;
BEGIN
  FOR i IN lower_bound .. upper_bound
  LOOP
    total:= total + i;
    IF i = lim THEN
      EXIT;
    END IF;
     -- Bounds are calculated only once.
     -- The below assignments have no effect on the loop condition
    lower_bound:= 900;
    upper_bound:= 1000;
  END LOOP;
  RETURN total;
END;
/
