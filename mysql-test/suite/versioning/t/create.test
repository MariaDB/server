  x29 int unsigned,
  Sys_end0 timestamp(6) as row end invisible,
  Sys_start timestamp(6) as row start invisible,
  Sys_end timestamp(6) as row end invisible,
  period for system_time (Sys_start, Sys_end)
) with system versioning;

