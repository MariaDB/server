CREATE TABLE t1(f1 INT NOT NULL, f2 INT NOT NULL,
		f3 INT, KEY(f1, f3), key(f3)) STATS_PERSISTENT=1,ENGINE=INNODB;
INSERT INTO t1 SELECT seq, seq, NULL from seq_1_to_16384;
