SET sql_mode=ORACLE;
#
# MDEV-10152 Add support for TYPE .. IS REF CURSOR
#
CREATE PROCEDURE p0 AS
BEGIN
IF @debug_ref_cursor IS TRUE
THEN
EXECUTE IMMEDIATE 'SHOW PROCEDURE CODE p1';
END IF;
CALL p1;
END;
$$
CREATE TABLE t1 (a INT);
INSERT INTO t1 VALUES (1);
CREATE TABLE t2 (a INT);
CREATE PROCEDURE p1 IS
CURSOR cursor_sample1 IS SELECT * FROM t1;
CURSOR cursor_sample2 IS SELECT * FROM t2;
/*empty*/
TYPE cur_t IS REF CURSOR RETURN cursor_sample1%ROWTYPE;
c0 cur_t;
v0 cursor_sample2%ROWTYPE;
BEGIN
OPEN c0 FOR SELECT * FROM t1;
FETCH c0 INTO v0;
CLOSE c0;
SELECT v0.a;
END;
$$
CALL p0;
v0.a
1
DROP PROCEDURE p1;
DROP TABLE t1, t2;
CREATE TABLE t1 (a INT, b VARCHAR(10));
INSERT INTO t1 VALUES (1, 'b1');
CREATE TABLE t2 (a INT, b VARCHAR(10));
CREATE PROCEDURE p1 IS
CURSOR cursor_sample1 IS SELECT * FROM t1;
CURSOR cursor_sample2 IS SELECT * FROM t2;
/*empty*/
TYPE cur_t IS REF CURSOR RETURN cursor_sample1%ROWTYPE;
c0 cur_t;
v0 cursor_sample2%ROWTYPE;
BEGIN
OPEN c0 FOR SELECT * FROM t1;
FETCH c0 INTO v0;
CLOSE c0;
SELECT v0.a, v0.b;
END;
$$
CALL p0;
v0.a	v0.b
1	b1
DROP PROCEDURE p1;
DROP TABLE t1, t2;
DROP PROCEDURE p0;
CREATE PROCEDURE p0 AS
BEGIN
