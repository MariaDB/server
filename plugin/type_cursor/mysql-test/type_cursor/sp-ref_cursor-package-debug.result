CREATE PROCEDURE p0 IS
BEGIN
IF @debug_ref_cursor IS TRUE
THEN
EXECUTE IMMEDIATE 'SHOW PROCEDURE CODE pkg1.p1';
END IF;
CALL pkg1.p1;
END;
$$
