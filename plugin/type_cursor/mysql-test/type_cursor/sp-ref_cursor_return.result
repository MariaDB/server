SET sql_mode=ORACLE;
#
# MDEV-10152 Add support for TYPE .. IS REF CURSOR
#
CREATE PACKAGE pkg1 AS
PROCEDURE p1;
END;
$$
CREATE PACKAGE BODY pkg1 AS
TYPE rec0_t IS RECORD (a INT, b TIME);
TYPE cur0_t IS REF CURSOR RETURN rec0_t;
FUNCTION f1 RETURN cur0_t AS
BEGIN
RETURN 10;
END;
PROCEDURE p1 AS
BEGIN
SELECT f1();
END;
END;
$$
CALL pkg1.p1;
ERROR HY000: Cannot cast 'int' as 'sys_refcursor' in assignment of `f1()` for operation SET
DROP PACKAGE pkg1;
CREATE PACKAGE pkg1 AS
PROCEDURE p1;
END;
$$
CREATE PACKAGE BODY pkg1 AS
TYPE rec0_t IS RECORD (a INT, b TIME);
TYPE cur0_t IS REF CURSOR RETURN rec0_t;
FUNCTION f1 RETURN cur0_t AS
BEGIN
RETURN @a;
END;
PROCEDURE p1 AS
BEGIN
SELECT f1();
END;
END;
$$
SET @a=10;
CALL pkg1.p1;
