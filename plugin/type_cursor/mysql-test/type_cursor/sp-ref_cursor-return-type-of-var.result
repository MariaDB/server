SET sql_mode=ORACLE;
#
# MDEV-10152 Add support for TYPE .. IS REF CURSOR
#
#
# Non-Oracle style: %TYPE of an explicit ROW variable
#
CREATE PROCEDURE p1 IS
v0 ROW(a INT, b VARCHAR(10));
TYPE cur0_t IS REF CURSOR RETURN v0%TYPE;
c0 cur0_t;
BEGIN
OPEN c0 FOR SELECT 1,'2 ' FROM DUAL;
FETCH c0 INTO v0;
SELECT LENGTH(v0.a) || ' ' || LENGTH(v0.b) || ' <'|| v0.a || '><'|| v0.b || '>' AS line;
CLOSE c0;
END;
$$
CALL p1;
line
1 2 <1><2 >
DROP PROCEDURE p1;
#
# TYPE cur0_t IS REF CURSOR RETURN v0%TYPE
# where v0 is of an explicit RECORD
