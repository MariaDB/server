# Error detected during CALL type
# Related to wrong data types in OPEN

SET sql_mode=ORACLE;

--echo #
--echo # MDEV-10152 Add support for TYPE .. IS REF CURSOR
--echo #

DELIMITER $$;
CREATE PROCEDURE p0 IS
BEGIN
  IF @debug_ref_cursor IS TRUE THEN
    EXECUTE IMMEDIATE 'SHOW PROCEDURE CODE p1';
  END IF;
  CALL p1;
END;
$$
DELIMITER ;$$


--echo # Explicit RECORD + element count mismatch

CREATE TABLE t1 (a INT, b VARCHAR(10), c VARCHAR(10));
INSERT INTO t1 VALUES (1,'b1','c1');
DELIMITER $$;
CREATE PROCEDURE p1 IS
  TYPE rec0_t IS RECORD (a INT, b VARCHAR(10));
  TYPE cur0_t IS REF CURSOR RETURN rec0_t;
  c0 cur0_t;
  v0 rec0_t;
BEGIN
  OPEN c0 FOR SELECT * FROM t1;
END;
$$
DELIMITER ;$$
--error ER_CANNOT_CAST_ON_IDENT1_ASSIGNMENT_FOR_OPERATION
CALL p0;
DROP PROCEDURE p1;
DROP TABLE t1;


CREATE TABLE t1 (a INT, b VARCHAR(10), c VARCHAR(10));
INSERT INTO t1 VALUES (1,'b1','c1');
DELIMITER $$;
CREATE PROCEDURE p1 IS
  TYPE rec0_t IS RECORD (a INT, b VARCHAR(10));
  r0 rec0_t;
  TYPE cur0_t IS REF CURSOR RETURN r0%TYPE;
  c0 cur0_t;
  v0 rec0_t;
BEGIN
  OPEN c0 FOR SELECT * FROM t1;
END;
$$
DELIMITER ;$$
--error ER_CANNOT_CAST_ON_IDENT1_ASSIGNMENT_FOR_OPERATION
CALL p0;
DROP PROCEDURE p1;
DROP TABLE t1;



