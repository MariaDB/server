CREATE OR REPLACE AGGREGATE FUNCTION aggregate_min_inet6(x INET6) RETURNS INET6
BEGIN
DECLARE res INET6 DEFAULT NULL;
DECLARE CONTINUE HANDLER FOR NOT FOUND
RETURN res;
LOOP
FETCH GROUP NEXT ROW;
IF (res IS NULL) OR (res > x) THEN
SET res= x;
END IF;
END LOOP;
END;
